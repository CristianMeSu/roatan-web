---
import Layout from "../../layouts/Layout.astro";
import Header from "../../components/Header/Header.astro";
export function getStaticPaths() {
  return [{ params: { lang: "es" } }, { params: { lang: "en" } }];
}
const lang = Astro.params.lang || "es";
---
<Layout lang={lang}>
  <Header lang={lang}  />
</Layout>
 